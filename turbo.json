{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "codegen"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "codegen": {
      "dependsOn": [],
      "outputs": ["codegen/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    }
  }
}
